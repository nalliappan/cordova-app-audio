<?xml version='1.0' encoding='utf-8'?>
<widget id="com.exelerus.apps.audioinputdemo" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="AudioInputCapture">
        <param name="ios-package" value="CDVAudioInputCapture" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>AudioInputDemo</name>
    <description>
        A sample Apache Cordova application that demonstrates the cordova-plugin-audioinput plugin.
    </description>
    <author email="exelerus.apps@gmail.com" href="https://exelerus.com">
        exelerus
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon src="./res/icon/ios/icon-180x180.png" />
    <icon src="./res/icon/ios/icon-120x120.png" />
    <icon src="./res/icon/ios/icon-60x60.png" />
    <icon src="./res/icon/ios/icon-76x76.png" />
    <icon src="./res/icon/ios/icon-152x152.png" />
    <icon src="./res/icon/ios/icon-40x40.png" />
    <icon src="./res/icon/ios/icon-80x80.png" />
    <icon src="./res/icon/ios/icon-57x57.png" />
    <icon src="./res/icon/ios/icon-114x114.png" />
    <icon src="./res/icon/ios/icon-72x72.png" />
    <icon src="./res/icon/ios/icon-144x144.png" />
    <icon src="./res/icon/ios/icon-29x29.png" />
    <icon src="./res/icon/ios/icon-58x58.png" />
    <icon src="./res/icon/ios/icon-50x50.png" />
    <icon src="./res/icon/ios/icon-100x100.png" />
    <icon src="./res/icon/ios/icon-167x167.png" />
    <splash src="./res/screen/ios/screen-320x480.png" />
    <splash src="./res/screen/ios/screen-640x960.png" />
    <splash src="./res/screen/ios/screen-768x1024.png" />
    <splash src="./res/screen/ios/screen-1536x2048.png" />
    <splash src="./res/screen/ios/screen-1024x768.png" />
    <splash src="./res/screen/ios/screen-2048x1536.png" />
    <splash src="./res/screen/ios/screen-640x1136.png" />
    <splash src="./res/screen/ios/screen-750x1334.png" />
    <splash src="./res/screen/ios/screen-1242x2208.png" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
</widget>
